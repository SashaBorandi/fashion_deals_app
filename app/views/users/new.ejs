<h1>New User</h1>

<form action="/users" method="post">
  <input type="hidden" name="_csrf" value="<%= csrfToken %>">
  <label>Email:
    <input type="text" name="user[email]">
  </label>
  <label>Password:
    <input type="text" name="user[password]">
  </label>
  <input type="submit" value="Register">
</form>
